<container id='meownatic_%{name}' size='0,80' color='#00000040' hover-color='#00000080'> 
    <label pos='78,6'>%{name}</label> 
    <label pos='80,28' size='455,50' multiline='true' color='#FFFFFFB2'> 
        %{description} 
    </label> 
    <label color='#CCFFE5B2' size='300,20' align='right'  
           gravity='bottom-right'> 
        version: %{version} 
    </label>
    <button margin='4'
            gravity='center-right'
            color='#00000000'
            hover-color='#FFFFFF2B'
            padding='0'
            onclick='del_meownatic("%{name}")'>
        <image src='gui/delete_icon' size='32,32' color='#FFFFFF50'/>
    </button>
    <image pos='8,8' src='menu/icons/%{icon}' size='64'/>
</container>
